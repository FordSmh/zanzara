{
    "name": "mosquito/mosquito",
    "type": "library",
    "description": "Library to build Telegram bots",
    "keywords": [
        "telegram",
        "bot"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Righetti"
        },
        {
            "name": "Mattia Corradi"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
