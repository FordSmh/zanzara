{
    "name": "badfarm/zanzara",
    "type": "library",
    "description": "Asynchronous PHP Telegram Bot Framework",
    "keywords": [
        "telegram",
        "bot",
        "reactphp",
        "async",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Righetti"
        },
        {
            "name": "Mattia Corradi"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-readline": "*",
        "psr/log": "^1.1",
        "psr/container": "^1.0",
        "php-di/php-di": "^6.1",
        "react/event-loop": "^1.1",
        "react/http": "^0.8.6",
        "react/react": "^1.0",
        "react/cache": "^1.0.0",
        "clue/buzz-react": "^2.7",
        "php-http/multipart-stream-builder": "^1.1",
        "netresearch/jsonmapper": "^2.0",
        "react/filesystem": "^0.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/dotenv": "^5.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Zanzara\\": "src/Zanzara"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zanzara\\Test\\": "tests"
        }
    }
}
