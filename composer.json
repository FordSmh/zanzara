{
    "name": "zanzara/zanzara",
    "type": "library",
    "description": "Library to build Telegram bots",
    "keywords": [
        "telegram",
        "bot",
        "reactphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Righetti"
        },
        {
            "name": "Mattia Corradi"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "netresearch/jsonmapper": "^2.0",
        "symfony/cache": "^5.0",
        "amphp/amp": "^2.4",
        "amphp/http-client": "^4.2",
        "amphp/parallel": "^1.3",
        "react/event-loop": "^1.1",
        "react/http": "^0.8.6",
        "react/react": "1.0",
        "clue/buzz-react": "2.7",
        "guzzlehttp/guzzle": "~6.0",
        "react/cache": "1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/dotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Zanzara\\": "src/Zanzara"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zanzara\\Test\\": "tests"
        }
    }
}
